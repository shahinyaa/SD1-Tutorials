total = 0  
count = 0  

# Initialize score 
score = 0


while score != -9:
    score = int(input("Enter score (Enter -9 to end): "))
    if score == -9:
        break
    total += score
    count += 1
if count > 0:
    average = float(total) / count
    print("Class average is", average)
else:
    print("No scores were entered. Cannot calculate the average.")